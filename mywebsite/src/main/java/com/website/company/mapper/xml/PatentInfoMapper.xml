<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.website.company.mapper.PatentInfoMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.website.company.entity.PatentInfo">
        <id column="id" property="id"/>
        <result column="apply_date" property="applyDate"/>
        <result column="patent_code" property="patentCode"/>
        <result column="patent_name" property="patentName"/>
        <result column="patentee" property="patentee"/>
        <result column="pic" property="pic"/>
        <result column="title" property="title"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,apply_date,patent_code,patent_name,patentee,pic,title
    </sql>

</mapper>
